@{
    ViewData["Title"] = "Upload PDF";
}

<h2>Upload a PDF File</h2>

<form method="post" enctype="multipart/form-data">
    <div>
        <label for="file">Choose PDF file to upload:</label>
        <input type="file" name="file" />
    </div>
    <div>
        <button type="submit">Upload PDF</button>
    </div>
</form>
